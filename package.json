{
  "name": "@travi/eslint-config-cucumber",
  "description": "Shareable config for ESLint for Cucumber.js tests",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "travi/eslint-config-cucumber",
  "bugs": "https://github.com/travi/eslint-config-cucumber/issues",
