  "devDependencies": {
    "@travi/eslint-config": "3.0.34",
    "ban-sensitive-files": "1.9.19",
    "commitlint-config-travi": "1.4.33",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "6.0.0"
  },
  "dependencies": {
    "@form8ion/eslint-config-cucumber": "1.4.1"
  }
